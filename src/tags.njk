---
permalink: /tags/
layout: base.njk
eleventyExcludeFromCollections: true
filter:
- posts
---
<h1>Tags</h1>

<ul>
{% for tag in collections.all | getAllTags %}
  {% set tagUrl %}/tags/{{ tag | slugify }}/{% endset %}
  <li style="height: 24px; line-height: 24px;"><a href="{{ tagUrl }}" class="post-tag">{{ tag }}</a></li>
{% endfor %}
</ul>
