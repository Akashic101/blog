{
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "all",
	"printWidth": 100,
	"jsxSingleQuote": false,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "strict",
	"vueIndentScriptAndStyle": false,
	"embeddedLanguageFormatting": "auto"
}
